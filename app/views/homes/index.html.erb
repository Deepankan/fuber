<script src="//maps.google.com/maps/api/js?key=AIzaSyDZmAW8QDL8yEutvQJCxZKQ2pTZ6Uz7xVI"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
<%= flash[:error] %>
<%= form_tag get_car_path, remote: true  do |k|%>
<label>Source</label>
<%= text_field_tag :start_location,nil, placeholder: "Enter Complete Location"%>
<label>Destination</label>
<%= text_field_tag :end_location,nil, placeholder: "Enter Destination Location"%>
<label>Is Pink?</label>
   <%= check_box_tag :is_pink %>
<%= submit_tag "Search Car"%>

<%end%>
<br />
<div id= "render_map" style='width: 800px;'>
  <%= render partial: "google_map"%><br />

</div>
<%= link_to "Start Trip", "#",:remote => true, id: "start_trip"%>  
<%= link_to "End Trip", "#",:remote => true, id: "end_trip"%> 
<script type="text/javascript">
  $('#start_trip').hide();
  $('#end_trip').hide();

	
</script>